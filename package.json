{
  "name": "yos-server",
  "version": "0.0.1",
  "description": "Your open source server",
  "keywords": [
    "server",
    "open source",
    "GraphQL",
    "Typescript",
    "Mongoose",
    "Typegoose"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist && node_modules/.bin/tsc",
    "dev": "rm -rf dist && node_modules/.bin/tsc-watch --onFirstSuccess 'node_modules/.bin/pm2 start pm2.config.js'",
    "lint": "node_modules/.bin/tslint -p tsconfig.json -c tslint.json 'src/**/*ts'",
    "log": "node_modules/.bin/pm2 log",
    "pack": "rm-rf ./*.tgz && npm run build && npm pack",
    "start": "npm run build && pm2 flush && node_modules/.bin/pm2 start pm2.config.js && node_modules/.bin/pm2 log yos-server",
    "start:prod": "npm run build && pm2 flush && node_modules/.bin/pm2 start pm2.config.js --env production && node_modules/.bin/pm2 log yos-server",
    "status": "node_modules/.bin/pm2 status",
    "stop": "node_modules/.bin/pm2 delete yos-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yos-tools/server.git"
  },
  "author": "Kai Haase",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yos-tools/server/issues"
  },
  "homepage": "https://yos.tools/server",
  "dependencies": {
    "@types/globby": "6.1.0",
    "@types/lodash": "4.14.109",
    "@types/node": "10.3.2",
    "apollo-server": "2.0.0-rc.0",
    "globby": "8.0.1",
    "graphql": "0.13.2",
    "lodash": "4.17.10",
    "mongoose": "5.1.5",
    "pm2": "2.10.4",
    "reflect-metadata": "0.1.12",
    "tsc-watch": "1.0.22",
    "tslint": "5.10.0",
    "typegoose": "5.2.1",
    "typescript": "2.9.1"
  }
}
