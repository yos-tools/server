{
  "name": "@yos.tools/server",
  "version": "0.0.1",
  "description": "Your open source server",
  "keywords": [
    "server",
    "open source",
    "Node",
    "GraphQL",
    "Express",
    "Typescript",
    "Mongoose",
    "Typegoose",
    "Apollo"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "lint": "tslint -p tsconfig.json -c tslint.json 'src/**/*ts'",
    "log": "pm2 log yos-server",
    "postinstall": "node_modules/.bin/pm2 install typescript",
    "reinit": "rm package-lock.json && rm -rf node_modules && npm cache clean --force && npm i",
    "start": "TS_NODE_FILES=true pm2 restart pm2-dev.config.js && pm2 log yos-server --lines 0",
    "start:prod": "npm run build && pm2 restart pm2-prod.config.js --env production && pm2 log yos-server --lines 0",
    "start:debug": "TS_NODE_FILES=true ts-node src/start.ts",
    "status": "pm2 status",
    "stop": "pm2 delete yos-server",
    "typedocs": "typedoc --excludeExternals --externalPattern \"**/node_modules/**\" --ignoreCompilerErrors --out typedocs src && opener typedocs/index.html",
    "zip": "rm-rf ./*.tgz && npm run build && npm pack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yos-tools/server.git"
  },
  "author": "Kai Haase",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yos-tools/server/issues"
  },
  "homepage": "https://yos.tools/server",
  "dependencies": {
    "@okgrow/graphql-scalars": "0.4.1",
    "@types/bcrypt": "2.0.0",
    "@types/cors": "2.8.4",
    "@types/express": "4.16.0",
    "@types/get-port": "4.0.0",
    "@types/globby": "8.0.0",
    "@types/graphql": "14.0.0",
    "@types/jsonwebtoken": "7.2.8",
    "@types/lodash": "4.14.116",
    "@types/node": "10.9.4",
    "@types/request": "2.47.1",
    "@types/request-promise-native": "1.0.15",
    "@types/validator": "9.4.2",
    "@types/ws": "6.0.1",
    "apollo-server": "2.0.8",
    "apollo-server-express": "2.0.7",
    "bcrypt": "3.0.0",
    "express": "4.16.3",
    "fortune": "5.5.0",
    "fortune-mongodb": "1.2.0",
    "get-port": "4.0.0",
    "globby": "8.0.1",
    "graphql": "0.13.2",
    "graphql-genie": "0.4.14",
    "graphql-genie-authentication": "1.1.5",
    "graphql-genie-subscriptions": "1.0.10",
    "graphql-s2s": "0.17.1",
    "graphql-tools": "3.1.1",
    "jsonwebtoken": "8.3.0",
    "lodash": "4.17.11",
    "opener": "1.5.1",
    "pm2": "3.1.2",
    "reflect-metadata": "0.1.12",
    "request": "2.88.0",
    "request-promise-native": "1.0.5",
    "tslint": "5.11.0",
    "typedoc": "0.12.0",
    "typescript": "3.0.3",
    "validator": "10.7.1",
    "ws": "6.0.0"
  }
}
