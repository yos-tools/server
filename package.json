{
  "name": "@yos.tools/server",
  "version": "0.0.1",
  "description": "Your open source server",
  "keywords": [
    "server",
    "open source",
    "Node",
    "GraphQL",
    "Express",
    "Typescript",
    "Mongoose",
    "Typegoose",
    "Apollo"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "lint": "tslint -p tsconfig.json -c tslint.json 'src/**/*ts'",
    "log": "pm2 log yos-server",
    "postinstall": "node_modules/.bin/pm2 install typescript",
    "reinit": "rm package-lock.json && rm -rf node_modules && npm cache clean --force && npm i",
    "start": "pm2 restart pm2-dev.config.js && pm2 log yos-server --lines 0",
    "start:prod": "npm run build && pm2 restart pm2-prod.config.js --env production && pm2 log yos-server --lines 0",
    "start:debug": "ts-node src/start.ts",
    "status": "pm2 status",
    "stop": "pm2 delete yos-server",
    "zip": "rm-rf ./*.tgz && npm run build && npm pack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yos-tools/server.git"
  },
  "author": "Kai Haase",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yos-tools/server/issues"
  },
  "homepage": "https://yos.tools/server",
  "dependencies": {
    "@types/cors": "^2.8.4",
    "@types/express": "4.16.0",
    "@types/get-port": "3.2.0",
    "@types/globby": "8.0.0",
    "@types/graphql": "0.13.1",
    "@types/lodash": "4.14.110",
    "@types/node": "10.3.5",
    "@types/ws": "5.1.2",
    "apollo-server-express": "2.0.0-rc.2",
    "express": "4.16.3",
    "get-port": "3.2.0",
    "globby": "8.0.1",
    "graphql": "0.13.2",
    "graphql-s2s": "^0.16.5",
    "lodash": "4.17.10",
    "mongoose": "5.1.6",
    "pm2": "2.10.4",
    "reflect-metadata": "0.1.12",
    "tslint": "5.10.0",
    "typegoose": "5.2.1",
    "typescript": "2.9.2",
    "ws": "5.2.1"
  }
}
