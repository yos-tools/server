{
  "name": "@yos.tools/server",
  "version": "0.0.1",
  "description": "Your open source server",
  "keywords": [
    "server",
    "open source",
    "Node",
    "GraphQL",
    "Express",
    "Typescript",
    "Mongoose",
    "Typegoose",
    "Apollo"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "lint": "tslint -p tsconfig.json -c tslint.json 'src/**/*ts'",
    "log": "pm2 log yos-server",
    "postinstall": "node_modules/.bin/pm2 install typescript",
    "reinit": "rm package-lock.json && rm -rf node_modules && npm cache clean --force && npm i",
    "start": "TS_NODE_FILES=true pm2 restart pm2-dev.config.js && pm2 log yos-server --lines 0",
    "start:prod": "npm run build && pm2 restart pm2-prod.config.js --env production && pm2 log yos-server --lines 0",
    "start:debug": "TS_NODE_FILES=true ts-node src/start.ts",
    "status": "pm2 status",
    "stop": "pm2 delete yos-server",
    "typedocs": "typedoc --excludeExternals --externalPattern \"**/node_modules/**\" --ignoreCompilerErrors --out typedocs src && opener typedocs/index.html",
    "zip": "rm-rf ./*.tgz && npm run build && npm pack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yos-tools/server.git"
  },
  "author": "Kai Haase",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yos-tools/server/issues"
  },
  "homepage": "https://yos.tools/server",
  "dependencies": {
    "@okgrow/graphql-scalars": "0.4.5",
    "@types/bcrypt": "3.0.0",
    "@types/cors": "2.8.4",
    "@types/express": "4.16.1",
    "@types/get-port": "4.0.1",
    "@types/globby": "8.0.0",
    "@types/graphql": "14.0.7",
    "@types/jsonwebtoken": "8.3.2",
    "@types/lodash": "4.14.122",
    "@types/node": "11.10.5",
    "@types/request": "2.48.1",
    "@types/request-promise-native": "1.0.15",
    "@types/validator": "10.9.0",
    "@types/ws": "6.0.1",
    "apollo-server": "2.4.8",
    "apollo-server-express": "2.4.8",
    "bcrypt": "3.0.4",
    "express": "4.16.4",
    "fortune": "5.5.17",
    "fortune-mongodb": "1.3.2",
    "get-port": "4.2.0",
    "globby": "9.1.0",
    "graphql": "14.1.1",
    "graphql-genie": "0.4.21",
    "graphql-genie-authentication": "1.1.6",
    "graphql-genie-subscriptions": "1.0.12",
    "graphql-s2s": "0.19.1",
    "graphql-tools": "4.0.4",
    "jsonwebtoken": "8.5.0",
    "lodash": "4.17.11",
    "opener": "1.5.1",
    "pm2": "3.3.1",
    "reflect-metadata": "0.1.13",
    "request": "2.88.0",
    "request-promise-native": "1.0.7",
    "ts-node": "8.0.3",
    "tslint": "5.13.1",
    "typedoc": "0.14.2",
    "typescript": "3.3.3333",
    "validator": "10.11.0",
    "ws": "6.2.0"
  }
}
